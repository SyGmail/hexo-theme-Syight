<div class="article-list">
  <ul>
    <% page.posts.each((post) => { %>
      <li>
        <a href="<%- url_for(post.path) %>">
          <h2>
            <%- post.title %>
          </h2>
          <p>发布时间: <%- post.date.format('L') %></p>
          <p>更新时间: <%- post.updated.format('L') %></p>
        </a>
      </li>
    <% }) %>
  </ul>
</div>
